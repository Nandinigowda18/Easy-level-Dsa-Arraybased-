package javaproject;
import java.util.*;

public class Tcs3 {

	
	public static int  fact(int n) {
//		if(n<=2) {
//			return n;
//		}
//		else {
//			return n*fact(n-1);
//		}
		
		//without recursion
		int fact=1;
		for(int i=1;i<=n;i++) {
			fact*=i;
		}
		return fact;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc= new Scanner(System.in);
		int n= sc.nextInt();
		
		
		System.out.println(fact(n));

	}

}
